{
  "name": "my-react-dev-box",
  "version": "1.0.0",
  "packageManager": "npm@11.0.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "test": "turbo test",
    "gen-pkg": "node scripts/gen-pkg.js",
    "format-fresh-app": "node scripts/format-fresh-vite-react-app.js",
    "update:check": "ncu",
    "update:check:minor": "ncu -u --target minor",
    "update:check:major": "ncu -u --target greatest",
    "update:check:interactive": "ncu -i"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.21.0",
    "@eslint/js": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "chalk": "^5.4.0",
    "eslint": "^9.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-oxlint": "^0.15.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "fs-extra": "^11.2.0",
    "globals": "^15.14.0",
    "inquirer": "^12.2.0",
    "ora": "^8.1.1",
    "react-refresh": "^0.16.0",
    "styled-components": "^6.1.13",
    "turbo": "^2.3.3",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "@scope/example": "^1.0.0",
    "dotenv": "^16.4.7",
    "npm-check-updates": "^17.1.11",
    "picocolors": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": "^20",
    "npm": "^10"
  }
}
